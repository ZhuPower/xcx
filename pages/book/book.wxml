<!--pages/book/book.wxml-->

<view>
  <scroll-view scroll-y="true" style="height: 600rpx;" bindscrolltoupper="upper" bindscrolltolower="lower">
    <view class="list" wx:for="{{bookList}}" wx:key="unique" data-item="{{item}}" catchtap="playlike" data-id="{{item.id}}">{{item.title}}</view>
  </scroll-view>
</view>
