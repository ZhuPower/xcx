/* pages/videoList/videoList.wxss */
.h2 {
  font-weight: bold;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  clear: both;
  text-indent: 2em;
}

.listUl {
  width: 100%;
  overflow: hidden;
}

.listUl .list {
  width: 32%;
  float: left;
  margin-left: 1%;
  height: 500rpx;
  margin-bottom: 1%;
}
.listUl .list image{
  width: 100%;
  height: 400rpx;
}
.listUl .list text{
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  font-size: 24rpx;
  display: block;
}

.classify {
  width: 100%;
  height: auto;
}

.classify .typeStatus,
.classify .list-type {
  width: 100%;
  overflow: hidden;
}

.typename {
  width: 20%;
  height: 100rpx;
  line-height: 100rpx;
  float: left;
  text-align: center;
}

.typename.cur {
  color: red;
}

.moreBtn {
  width: 100%;
  margin: 40rpx auto;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}